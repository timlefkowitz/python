# python Palindrome(s): okay, s[] must mean its an array? Looking this up now
# variable[::10] 
def aBooleanPalindrome
    return s == s[::-1]

# Driver Code
s = "malayalam"
 xyz = isPalindrome(s)

if xyz: 
    print("yes")
else: 
    print("no")



def isPalindrome(str):

    # Run loop from 0 to len/2
    for i in range (0, int(len(str)/2)):
        if str[i] != str(len(str)-i-1):
            return False
        return True

    # main function 
    string = "loopydoopl"
    yxz = isPalindrome(string)

    if(string):
        print("Yes")
        else:
            print("No")    

# Method using recursion

def isPalindome(string):

    #to change it the string is similar case
    s = s.lower()
    # length of s
    l = len(s)

    # if length is less than 2
    if l < 2:
        return True

    # If s[0] and s[l - 1]:
    elif s[0] == s[1 - 1]:

        # Call is pallindrome form substring(1,1-1)
        return isPalindrome(s[1: 1 - 1])

        else:
            return False

    # Driver Code 
    string = "Loopydooply"
    xyz = isPalindrome(s)

    if xyz:
        print("Yes")

        else:
            print("No")            